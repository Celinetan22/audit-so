generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Audit {
  id            Int      @id @default(autoincrement())
  pic           Json
  bulan         String
  minggu        String?
  tanggal       String?
  jabodetabek   String?
  luarJabodetabek String?
  cabang        String?
  warehouse     String?
  tradisional   String?
  modern        String?
  whz           String?
  notes         String?
  status        String?
  createdAt     DateTime @default(now())
}

